<?sap.transform simple?>
<tt:transform xmlns:tt="http://www.sap.com/transformation-templates" xmlns:ddic="http://www.sap.com/abapxml/types/dictionary" xmlns:def="http://www.sap.com/abapxml/types/defined">
  <tt:root name="SNODETAB" type="ddic:SNODETAB"/>
  <tt:template>
      <CLASSTREE>
      <tt:loop ref=".SNODETAB">
        <SNODETEXT>
          <ID tt:value-ref="ID"/>
          <TYPE tt:value-ref="TYPE"/>
          <NAME tt:value-ref="NAME"/>
          <PARENT tt:value-ref="PARENT"/>
          <CHILD tt:value-ref="CHILD"/>
          <TEXT1 tt:value-ref="TEXT1"/>
          <TEXT2 tt:value-ref="TEXT2"/>
          <TEXT8 tt:value-ref="TEXT8"/>
          <TEXT9 tt:value-ref="TEXT9"/>
        </SNODETEXT>
      </tt:loop>
    </CLASSTREE>
  </tt:template>
</tt:transform>
